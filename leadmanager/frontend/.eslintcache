dispatch({ type: GET_LEADS, payload: res.data });\n    })\n    .catch((err) =>\n      dispatch(returnErrors(err.response.data, err.response.status))\n    );\n};\n\n//Delete Lead\nexport const deleteLead = (id) => (dispatch, getState) => {\n  axios\n    .delete(`http://localhost:8000/api/leads/${id}`, tokenConfig(getState))\n    .then((res) => {\n      dispatch(createMessage({ deleteLead: 'Lead Deleted' }));\n      dispatch({ type: DELETE_LEAD, payload: id });\n    })\n    .catch((err) => console.log(err));\n};\n\n//Add Lead\nexport const addLead = (user) => (dispatch, getState) => {\n  axios\n    .post('http://localhost:8000/api/leads/', user, tokenConfig(getState))\n    .then((res) => {\n      dispatch(createMessage({ addLead: 'Lead Added' }));\n      dispatch({\n        type: ADD_LEAD,\n        payload: res.data,\n      });\n    })\n    .catch((err) =>\n      dispatch(returnErrors(err.response.data, err.response.status))\n    );\n};\n",["99","100"],"/home/n1ck-lucky/Desktop/Github/Django-React/leadmanager/frontend/src/components/layout/Alerts.js",[],"/home/n1ck-lucky/Desktop/Github/Django-React/leadmanager/frontend/src/reducers/errors.js",[],"/home/n1ck-lucky/Desktop/Github/Django-React/leadmanager/frontend/src/reducers/messages.js",[],"/home/n1ck-lucky/Desktop/Github/Django-React/leadmanager/frontend/src/actions/messages.js",[],"/home/n1ck-lucky/Desktop/Github/Django-React/leadmanager/frontend/src/actions/errors.js",[],"/home/n1ck-lucky/Desktop/Github/Django-React/leadmanager/frontend/src/components/accounts/Register.js",[],"/home/n1ck-lucky/Desktop/Github/Django-React/leadmanager/frontend/src/components/accounts/Login.js",[],"/home/n1ck-lucky/Desktop/Github/Django-React/leadmanager/frontend/src/reducers/auth.js",[],"/home/n1ck-lucky/Desktop/Github/Django-React/leadmanager/frontend/src/components/common/PrivateRoute.js",[],"/home/n1ck-lucky/Desktop/Github/Django-React/leadmanager/frontend/src/actions/auth.js",[],{"ruleId":"101","replacedBy":"102"},{"ruleId":"103"