rom 'react-redux';\nimport { logout } from '../../actions/auth';\n\nconst Header = (props) => {\n  const { isAuthenticated, user } = props.auth;\n\n  const guestLinks = (\n    <ul className='navbar-nav mt-2'>\n      <li className='nav-item'>\n        <Link to='/register' className='nav-link'>\n          Register\n        </Link>\n      </li>\n      <li className='nav-item'>\n        <Link to='/login' className='nav-link'>\n          Login\n        </Link>\n      </li>\n    </ul>\n  );\n\n  const authLinks = (\n    <div className='container d-flex justify-content-evenly p-1 align-items-center'>\n      <span className='navbar-text mr-3'>\n        <strong>{user ? `Welcome ${user.username}` : ``}</strong>\n      </span>\n      <button\n        onClick={props.logout}\n        className='ml-3 text-light btn btn-danger nav-link btn-sm'\n      >\n        Logout\n      </button>\n    </div>\n  );\n\n  return (\n    <div className='Header'>\n      <nav className='navbar navbar-expand-sm navbar-light bg-light'>\n        <div className='container d-flex justify-content-center align-items-center'>\n          <div className='container' id='navbarTogglerDemo01'>\n            <a className='navbar-brand' href=''>\n              Lead Manager\n            </a>\n          </div>\n          {isAuthenticated ? authLinks : guestLinks}\n        </div>\n      </nav>\n    </div>\n  );\n};\n\nconst mapStateToProps = (state) => ({\n  auth: state.auth,\n});\n\nexport default connect(mapStateToProps, { logout })(Header);\n","/home/n1ck-lucky/Desktop/Github/Django-React/leadmanager/frontend/src/components/leads/Dashboard.js",[],"/home/n1ck-lucky/Desktop/Github/Django-React/leadmanager/frontend/src/components/leads/Leads.js",["97"],"import React, { useEffect } from 'react';\nimport { connect } from 'react-redux';\nimport { getLeads, deleteLead } from '../../actions/leads';\n\nconst Leads = (props) => {\n  useEffect(() => {\n    props.getLeads();\n  }, []);\n  return (\n    <div className='Leads'>\